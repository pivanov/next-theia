{
  "compilerOptions": {
    "lib": [
      "dom",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "outDir": "./dist"
  },
  "include": [
    "src-gen/frontend/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
